<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ContentValidationError.
    -->
    <changeSet id="20191031122727-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="task_id"
                                 baseTableName="content_validation_error"
                                 constraintName="fk_content_validation_error_task_id"
                                 referencedColumnNames="id"
                                 referencedTableName="task"/>

        <addForeignKeyConstraint baseColumnNames="task_execution_id"
                                 baseTableName="content_validation_error"
                                 constraintName="fk_content_validation_error_task_execution_id"
                                 referencedColumnNames="id"
                                 referencedTableName="task_execution"/>

    </changeSet>
</databaseChangeLog>
